{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "binarytorch/larecipe": "^2.2",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "laravel/ui": "^1.1",
        "laravelcollective/html": "^6.0",
        "predis/predis": "^1.1",
        "vof/ext.laravel.vof.admin": "1.0.4",
        "vof/ext.laravel.vof.usermanagment": "1.0.1",
        "vof/ext.laravel.vof.category": "dev-develop"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan package:discover --ansi",
            "php artisan key:generate"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories":[
        {
            "type": "vcs",
            "url" : "git@bitbucket.org-vof:vofvapeshop/ext.laravel.vof.admin.git"
        },
        {
            "type": "vcs",
            "url" : "git@bitbucket.org-vof:vofvapeshop/ext.laravel.vof.usermanagment.git"
        },
        {
            "type": "vcs",
            "url" : "git@bitbucket.org-vof:vofvapeshop/ext.laravel.vof.category.git"
        }
    ]
}
